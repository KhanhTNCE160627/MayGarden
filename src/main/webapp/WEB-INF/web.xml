    </servlet>
    <servlet>
        <servlet-name>AddCategoryController</servlet-name>
        <servlet-class>com.controllers.AddCategoryController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DetailAccountController</servlet-name>
        <servlet-class>com.controllers.DetailAccountController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadAccountController</servlet-name>
