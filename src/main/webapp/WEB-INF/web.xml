    </servlet>
    <servlet>
        <servlet-name>AdminProductY</servlet-name>
        <servlet-class>com.controllers.AdminProductY</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteProduct</servlet-name>
        <servlet-class>com.controllers.DeleteProduct</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateProduct</servlet-name>
        <servlet-class>com.controllers.CreateProduct</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditProductAdmin</servlet-name>
        <servlet-class>com.controllers.EditProductAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DetailProductAdmin</servlet-name>
        <servlet-class>com.controllers.DetailProductAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadProductController</servlet-name>
        <servlet-class>com.controllers.LoadProductController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginControllerV</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterControllerV</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutController</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchController</servlet-name>
        <url-pattern>/Search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeControllerV</servlet-name>
        <url-pattern>/Home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrderAdminV</servlet-name>
        <url-pattern>/Order</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductController</servlet-name>
        <url-pattern>/Product</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccountController</servlet-name>
        <url-pattern>/Account</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CategoryControllerV</servlet-name>
        <url-pattern>/Category</url-pattern>
